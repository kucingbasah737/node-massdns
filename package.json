{
  "name": "massdns",
  "version": "1.0.1",
  "description": "MassDNS wrapper for node.js",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "scripts": {
    "test": "mocha --bail --recursive test",
    "test-watch": "mocha --bail --watch --watch-ignore tmp/test --recursive test",
    "postversion": "git push && git push --tags",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "update-npmignore": "cat .gitignore npmignore-list > .npmignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kucingbasah737/node-massdns.git"
  },
  "keywords": [
    "massdns",
    "dns"
  ],
  "author": "Kucing Basah 737 <kucingbasah737@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kucingbasah737/node-massdns/issues"
  },
  "homepage": "https://github.com/kucingbasah737/node-massdns#readme",
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "mocha": "^10.2.0",
    "semistandard": "^17.0.0",
    "should": "^13.2.3"
  },
  "engines": {
    "node": ">=16"
  },
  "auto-changelog": {
    "commitUrl": "https://github.com/kucingbasah737/node-massdns/commit/{id}",
    "compareUrl": "https://github.com/kucingbasah737/node-massdns/compare/{from}...{to}",
    "issueUrl": "https://github.com/kucingbasah737/node-massdns/issues/{id}"
  },
  "dependencies": {
    "dayjs": "^1.11.10"
  }
}
